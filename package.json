{
  "name": "multi_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios-dev": "react-native run-ios --scheme=dev --configuration Debug",
    "ios-stag": "react-native run-ios --scheme=stag --configuration Debug",
    "ios-prod": "react-native run-ios --scheme=prod --configuration Debug",
    "ios-dev-release": "react-native run-ios --scheme=dev --configuration Release",
    "ios-stag-release": "react-native run-ios --scheme=stag --configuration Release",
    "ios-prod-release": "react-native run-ios --scheme=prod --configuration Release",
    "android-dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android-stag": "react-native run-android --variant=stagDebug --appIdSuffix=stag",
    "android-prod": "react-native run-android --variant=prodDebug",
    "android-dev-release-apk": "cd android && ./gradlew clean && ./gradlew assembleDevRelease",
    "android-stag-release-apk": "cd android && ./gradlew clean && ./gradlew assembleStagRelease",
    "android-prod-release-apk": "cd android && ./gradlew clean && ./gradlew assembleProdRelease",
    "android-dev-release-mode": "react-native run-android --variant=devRelease --appIdSuffix=dev",
    "android-stag-release-mode": "react-native run-android --variant=stagRelease --appIdSuffix=stag",
    "android-prod-release-mode": "react-native run-android --variant=prodRelease",
    "android-dev-release-aab": "cd android && ./gradlew clean && ./gradlew bundleDevRelease",
    "android-stag-release-aab": "cd android && ./gradlew clean && ./gradlew bundleStagRelease",
    "android-prod-release-aab": "cd android && ./gradlew clean && ./gradlew bundleProdRelease"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
